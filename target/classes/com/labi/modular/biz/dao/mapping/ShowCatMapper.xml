<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE mapper PUBLIC "-//mybatis.org//DTD Mapper 3.0//EN" "http://mybatis.org/dtd/mybatis-3-mapper.dtd">
<mapper namespace="com.labi.modular.biz.dao.ShowCatMapper">

    <!-- 通用查询映射结果 -->
    <resultMap id="BaseResultMap" type="com.labi.modular.biz.model.ShowCat">
        <id column="id" property="id" />
        <result column="name" property="name" />
        <result column="seq_num" property="seqNum" />
        <result column="status" property="status" />
        <result column="type" property="type" />
        <result column="createtime" property="createtime" />
        <result column="oprtime" property="oprtime" />
    </resultMap>

    <!-- 通用查询结果列 -->
    <sql id="Base_Column_List">
        id, name, seq_num AS seqNum, status,type, createtime, oprtime
    </sql>

</mapper>
